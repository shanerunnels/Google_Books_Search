{
  "name": "google-book-search",
  "version": "1.0.0",
  "description": "React google book search",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "Shane Runnels",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "gh-pages": "^3.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^2.1.8",
    "react-toastify": "^5.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lilysimmons/google-books-search"
  },
  "bugs": {
    "url": "https://github.com/lilysimmons/google-books-search/issues"
  },
  "homepage": "https://github.com/lilysimmons/google-books-search#readme"
}
